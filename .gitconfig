[user]
	name = AndrewBennettDev
	email = andrewbennettdev@gmail.com
[init]
  defaultBranch = main
[pull]
  rebase = true
[core]
  editor = vim
  browser = brave-browser
[web]
  browser = brave-browser
[alias]
  done = "!git add . && git commit -m 'checkpoint commit'"
  lg = log --pretty=\"format:%h %s\" --graph
  st = status -s
  alias = !git config --get-regexp 'alias.*' | colrm 1 6 | sed 's/[ ]/ = /'
